<%= simple_form_for(planet) do |f| %>
<%= f.input :name %>
<%= f.input :catch_phrase %>
<%= f.input :summary %>
<%= f.input :planet_type %>
<%= f.input :inhabitants %>
<%= f.input :available_places %>
<%= f.input :location %>
<%= f.input :price %>
<%= f.input :photo, as: :file %>
<%= f.submit 'Add a new list', class: 'btn btn-success' %>
<% end %>
